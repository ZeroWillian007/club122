        <div class="row panel panel-primary">
            <div class="panel-heading">
                <strong>EDITAR USUARIOS  </strong>
            </div>
            <div class="panel-body">
                 
                <form class="bg-info" method="POST" action="{{path('wbj_user_buscarusereditarporid')}}" enctype="multipart/form-data">
                   <div class="row">
                       <div class="col-md-4">
                            <div class="form-group">
                                <label>CORREO ELECTRONICO</label>
                               
                                <input class="form-control" value=" {{user.email}}  " name="correo" id="correo" />
                               
                            </div>
                           <button type="submit" class="btn btn-danger btn-block">ACEPTAR</button>
                           <a class="btn  btn-success btn-block" href="{{path('wbj_user_editarusernoadmin')}}">NUEVA CONSULTA</a>
                       </div>
                       <div class="col-md-8">
                         <div class="form-group">
                            
                            <input class="form-control" value="{{user.nombres}}" name="nombresa" id="nombresa" />
                        </div>
                        <div class="form-group">
                            <input class="form-control" value="{{user.apellidos}}" name="apellidosa" id="apellidosa" />
