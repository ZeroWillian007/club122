                        <div class="form-group">
                            <input class="form-control" value="{{user.provincia}}" name="provinciaa" id="provinciaa"/>
                        </div> 
                        <div class="form-group">
                            <input class="form-control" value="{{user.ciudad}}" name="ciudada" id="ciudada" />
                        </div> 
                        <div class="form-group">
                            <input class="form-control" value="{{user.introduc}}" name="introduca" id="introduca" />
                        </div>
                        
                        <!--FOTOS-->
                            <label class="btn btn-success btn-block">
                               FOTO PERFIL <input type="file" style="display: none;"  id="file-perfil" name="file-perfil" >
